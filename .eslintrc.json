{
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "rules": {
      "react/jsx-filename-extension": 0,
      "no-use-before-define": 0,
      "no-console": 0
    },
    "globals": {
      "console": true
    }
}